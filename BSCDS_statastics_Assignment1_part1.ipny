   "source": [
    "data=[5,8,None,7,10,None,6]\n",
    "z=0\n",
    "count=0\n",
    "for x in data:\n",
    "    if x is not None:\n",
    "        z+=x\n",
    "        count+=1\n",
    "mean_val=z/count\n",
    "imputed=[]\n",
    "for x in data:\n",
    "   if x is not None:\n",
    "     imputed.append(mean_val)\n",
    "else:\n",
    "    imputed.append(x)\n",
    "print(\"original data:\", data)\n",
    "print(\"imputed data:\", imputed)\n",
    "print(\"mean:\", mean_val)"
   ]
